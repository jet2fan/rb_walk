<?xml version="1.0"?>

<robot name="ri4">
  <material name="red">
    <color rgba="1.0 0.0 0.0 2.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 1.0 0.0 2.0"/>
  </material>
  <material name="blue">
    <color rgba="0.0 0.0 1.0 2.0"/>
