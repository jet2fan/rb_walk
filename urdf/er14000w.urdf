<?xml version="1.0"?>

<robot name="ri4">
  <material name="red">
    <color rgba="1.0 0.0 0.0 2.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 1.0 0.0 2.0"/>
  </material>
  <material name="blue">
    <color rgba="0.0 0.0 1.0 2.0"/>
  </material>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
	    <robotNamespace>ri4</robotNamespace>
    </plugin>
  </gazebo>

<!-- ################################################################### -->

    <link name="base_link"> </link>
    
<!-- ################################################################### -->

    <joint name="joint0" type="fixed">
      <parent link="base_link"/>
      <child link="body"/>
      <origin xyz= "1 1 1.2" rpy="0 0 0" />
    </joint>

<!-- doutai -->
    <link name="body">
      <visual>
        <!-- origin xyz="0 0 0" rpy="0 0 0"/ >
        < geometry> <box size="0.8 0.4 0.6"/> </geometry -->
        <origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry> <box size="0.2 0.1 0.5"/> </geometry>
        <material name="red"/>
      </visual>
      <collision>
        <origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry> <box size="0.2 0.1 0.5"/> <!-- box size="0.8 0.4 0.6"/ --> </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0.25" rpy="0 0 0"/> 
        <mass value="20"/>
        <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
      </inertial>
    </link>

<!-- ################################################################### -->

<!-- kokokara migiasi -->
<!-- migi kokansetu -->
  <joint name="joint1" type="revolute">
     <parent link="body"/>
     <child link="link1"/>
     <!-- origin xyz= "0 0 0.3" rpy="0 0 0" />
     <axis xyz="0 1 0"/ -->
     <origin xyz="0.1 0 0.0" rpy="0 0 0"/>
     <axis xyz="0 1 0"/>
     <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
   </joint>
<!-- ue sokeibu -->
    <link name="link1">
      <visual>
        <!-- origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry> <cylinder length="0.5" radius="0.1"/> </geometry -->
        <geometry> <box size="0.2 0.05 0.1"/> </geometry>
        <origin xyz="0.1 0 -0.05" rpy="0 0 0"/>
        <material name="blue"/>
      </visual>
      <collision>
        <!-- origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry> <cylinder length="0.5" radius="0.1"/> </geometry -->
        <geometry> <box size="0.2 0.05 0.1"/> </geometry>
        <origin xyz="0.1 0 -0.05" rpy="0 0 0"/>
      </collision>
      <inertial>
        <origin xyz="0.1 0 -0.05" rpy="0 0 0"/> <!-- origin xyz="0 0 0.25" rpy="0 0 0"/ --> 
        <mass value="0.1"/>
        <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
      </inertial>
    </link>
    <transmission name="trans1">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="joint1"> <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface> </joint>
      <actuator name="motor1">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

<!-- ################################################################### -->
<!-- migi kokansetu -->
  <joint name="joint2" type="revolute">
     <parent link="link1"/>
     <child link="link2"/>
     <!-- origin xyz= "0 0 0.5" rpy="0 0 0" / -->
     <origin xyz="0.1 0 -0.1" rpy="0 0 0"/>
     <axis xyz="1 0 0"/>
     <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

<!-- migi futomomo -->
    <link name="link2">
      <visual>
        <!-- origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry> <cylinder length="0.5" radius="0.1"/> </geometry -->
        <geometry> <box size="0.1 0.02 0.4"/> </geometry>
        <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      </visual>
      <collision>
        <!-- origin xyz="0 0 0.25" rpy="0 0 0"/ -->
        <origin xyz="0 0 -0.2" rpy="0 0 0"/>
        <!-- geometry> <cylinder length="0.5" radius="0.1"/> </geometry -->
        <geometry> <box size="0.1 0.02 0.4"/> </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 -0.2" rpy="0 0 0"/> <!-- origin xyz="0 0 0.25" rpy="0 0 0"/ --> 
        <mass value="0.1"/>
        <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/> 
      </inertial>
    </link>
    <transmission name="trans2">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="joint2">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="motor2">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

<!-- ################################################################### -->
<!-- migi hiza -->
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <!-- origin xyz= "0 0 0.5" rpy="0 0 0" / -->
    <origin xyz="0 0 -0.4" rpy="0 0 3.1415"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

<!-- migi sune -->
  <link name="link3">
    <visual>
      <!-- origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry> <box size="0.6 0.1 0.4"/> </geometry -->
      <geometry> <box size="0.1 0.01 0.4"/> </geometry>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <material name="red"/>
    </visual>
    <collision>
      <!-- origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry> <box size="0.6 0.1 0.4"/> </geometry -->
      <geometry> <box size="0.1 0.01 0.4"/> </geometry>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/> <!-- origin xyz="0 0 0.2" rpy="0 0 0"/ --> 
      <mass value="0.1"/>
      <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
    </inertial>
  </link>
  <!-- transmission name="trans3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint3">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission -->
  <transmission name="trans3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint3">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

<!-- ################################################################### -->
<!-- migi kurubusi kansetu -->
  <joint name="jointx" type="revolute">
    <parent link="link3"/>
    <child link="linkx"/>
    <!-- origin xyz= "0.4 0 0.3" rpy="0 0 0" / -->
    <origin xyz="0 0 -0.4" rpy="0 0 3.1415926"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

<!-- migi kurubusi -->
  <link name="linkx">
    <visual>
      <!-- origin xyz="0.2 0 0" rpy="0 0 0"/>
      <geometry> <box size="0.4 0.1 0.1"/> </geometry -->
      <geometry> <box size="0.2 0.05 0.1"/> </geometry>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    </visual>
    <collision>
      <!-- origin xyz="0.2 0 0" rpy="0 0 0"/>
      <geometry> <box size="0.4 0.1 0.1"/> </geometry -->
      <geometry> <box size="0.2 0.05 0.1"/> </geometry>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    </collision>
    <inertial>
      <!-- origin xyz="0.2 0 0" rpy="0 0 0"/ -->
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
    </inertial>
  </link>
  <transmission name="transx">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="jointx">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motorx">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

<!-- ################################################################### -->
<!-- migi kurubisi sita -->
  <joint name="jointx2" type="fixed">
    <parent link="linkx"/>
    <child link="linkx2"/>
    <!-- origin xyz= "0.4 0 0" rpy="0 0 0" / -->
    <origin xyz="0 0 -0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

<!-- migi asi ura -->
  <link name="linkx2">
    <visual>
      <!-- origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry> <box size="0.1 0.1 0.4"/> </geometry -->
      <geometry> <box size="0.2 0.3 0.01"/> </geometry>
      <origin xyz="0 0 -0.005" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <!-- origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry> <box size="0.1 0.1 0.4"/> </geometry -->
      <geometry> <box size="0.2 0.3 0.01"/> </geometry>
      <origin xyz="0 0 -0.005" rpy="0 0 0"/>
    </collision>
    <inertial>
      <!-- origin xyz="0 0 -0.2" rpy="0 0 0"/ -->
      <origin xyz="0 0 -0.005" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
    </inertial>
  </link>

<!-- ################################################################### -->
<!-- hidariasi sokeibu ue -->

  <joint name="jointy" type="revolute">
    <parent link="body"/>
    <child link="linky"/>
    <!-- origin xyz= "0 0.2 0.3" rpy="0 0 0" / -->
    <origin xyz="-0.1 0 0.0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

<!-- hidari sokeibu -->
  <link name="linky">
    <visual>
      <!-- origin xyz="0 0.4 0" rpy="0 0 0"/>
      <geometry> <box size="0.1 0.8 0.1"/> </geometry -->
      <geometry> <box size="0.2 0.05 0.1"/> </geometry>
      <origin xyz="-0.1 0 -0.05" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <!-- origin xyz="0 0.4 0" rpy="0 0 0"/>
      <geometry> <box size="0.1 0.8 0.1"/> </geometry -->
      <geometry> <box size="0.2 0.05 0.1"/> </geometry>
      <origin xyz="-0.1 0 -0.05" rpy="0 0 0"/>
    </collision>
    <inertial>
      <!-- origin xyz="0 0.4 0" rpy="0 0 0"/ -->
      <origin xyz="-0.1 0 -0.05" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="5e-2" ixy="0" ixz="0" iyy="5e-2" iyz="0" izz="5e-2"/>
    </inertial>
  </link>
  <transmission name="transy">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="jointy">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motory">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

</robot>
